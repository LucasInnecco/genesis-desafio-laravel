version: "3.7"
services:
  app:
    build:
      args:
        user: gen
        uid: 1000
      context: ./
      dockerfile: Dockerfile
    container_name: avaliacao-app
    restart: unless-stopped
    working_dir: /var/www/
    volumes:
      - ./:/var/www
    ports:
      - "9002:9000"
    networks:
      - avaliacao

networks:
  avaliacao:
    driver: bridge
